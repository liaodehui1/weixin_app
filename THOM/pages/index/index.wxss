.header{
  width: 100%;
  height: 120px;
  background-color: #1296db;
  box-sizing: border-box;
  padding: 0 5px;
}
.swiper{
  position: relative;
  width: 100%;
  margin: 0 auto;
  border-radius: 8px;
  overflow: hidden;
}
.slide-image{
  width: 100%;
  height: 100%;
  display: inline-block;
}
.programas{
  position: relative;
  margin-top: 30px;
  width: 100%;
  height: 100rpx;
  display: flex;
  padding-top: 10px;
  justify-content: center;
}
.programa{
  height: 100%;
  flex: 1;
  text-align: center;
}
.programa-img{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
}
.programa-title{
  padding-top: 5rpx;
  display: block;
  font-size: 10px;
}