swiper{
    height: 421.5rpx;
}
swiper-item image{
    width: 100%;
    height: 100%;
}
.select-title,.newest-title{
    width: 100%;
    padding: 30rpx 0;
    text-align: center;
    color: #ab956d;
}
.select-top,.select-bottom{
    width: 750rpx;
    height: 375rpx;
}
.select-top-small{
    width: 375rpx;
    height: 375rpx;
    float: left;
}
.select-top-small navigator,
.select-top-small image,
.select-bottom navigator,
.select-bottom image{
    width: 100%;
    height: 100%;
    display: block;
}
.newest-box{
    padding: 0 40rpx;
}
.newest-list{
    display: inline-block;
    width: 325rpx;
    height: 325rpx;
    margin: 0 10rpx 20rpx 0;
    border-radius: 10px;
    text-align: center;
    background-color: #f5f6f5;
}
.newest-list image{
    width: 175rpx;
    height: 175rpx;
    margin: 20rpx 0 10rpx;
}
.newest-text{
    font-size: 32rpx;
}